@INPROCEEDINGS{Fodischa,
  author = {F{\"o}disch, Mike and Winter, Andreas and Hermes, Thorsten},
  title = {Modellbasierte Raumlageerkennung von Objekten auf Tiefenbildern},
  booktitle = {3D-NordOst 2001, Tagungsband 4. Anwendungsbezogener Workshop zur
	Erfassung, Verarbeitung, Modellierung und Auswertung von 3D-Daten},
  year = {2001},
  editor = {Stanke, Gerd and Paul, Lothar and Pochanke, M.},
  pages = {5--12},
  address = {Berlin, Germany},
  publisher = {GFaI},
  abstract = {Im folgenden wird ein Ansatz zur modellbasierten Raumlagebestimmung
	von Objekten auf Tiefenbildern vorgestellt. Das Verfahren besteht
	aus einem Vorverarbeitungsteil, der die Eingangsdaten segmentiert,
	und einem Objekterkennungsteil, der die Szenenmerkmale den Modellmerkmalen
	zuordnet. Die Segmentierung nutzt die gesamten Daten des verwendeten
	3D-Sensors1, der neben dem Tiefenbild auch ein pixelidentisches Grauwertbild
	liefert. Die Objekterkennung verwendet eine Variante der beschr{\"\i}{\?}ï¿½kten
	Baumsuche2zur Zuordnung der Merkmale aus der Segmentierung zu Objektmerkmalen.
	Im Rahmen vorhergehender Arbeiten3 entstand ein Segmentierungsverfahren,
	das kanten- sowie regionenbasierte Segmentierungsans{\"\i}{\?}ï¿½ze
	kombiniert. Zun{\"\i}{\?}ï¿½hst wird sowohl auf das gegl{\"\i}{\?}ï¿½tete
	Tiefenbild als auch auf das Grauwertbild der Sobeloperator angewendet
	und dessen Ergebnisse binarisiert. Zu diesen beiden Merkmalsquellen
	wird eine weitere hinzugefgt. Das Tiefenbild wird in Bl{\"\i}{\?}ï¿½ke
	aufgeteilt, die mit einer planaren Funktion approximiert werden.
	Wenn der Approximationsfehler einen Schwellwert berschreitet, wird
	der Block als Kantenblock eingestuft. Aus diesen drei Merkmalsquellen
	werden die endgltigen Kantenbereiche ermittelt. Dazu wird ein Punkt
	als endgltiger Kantenpunkt markiert, wenn dieser in mindestens zwei
	der drei Merkmalsquellen als Kantenpunkt detektiert wurde bzw. zu
	einem Kantenblock geh{\"\i}{\?}ï¿½t. Anschlie{\"\i}{\?}ï¿½nd werden die
	Bl{\"\i}{\?}ï¿½ke, die nicht als Kantenbl{\"\i}{\?}ï¿½ke markiert wurden,
	zu Regionen verschmolzen. Diese Regionen werden in einem weiteren
	Schritt pixelweise in die Kantenbereiche ausgedehnt. Das Segmentierungsverfahren
	liefert eine Segmentbeschreibung, die aus der Kontur und einer Fl{\"\i}{\?}ï¿½henfunktion,
	welche die von der Kontur eingeschlossene Fl{\"\i}{\?}ï¿½he mathematisch
	approximiert, besteht. Die Fl{\"\i}{\?}ï¿½henfunktion ist eine Polynomfunktion
	maximal zweiten Grades. Im letzten Schritt erfolgt eine Konturpunktreduktion,
	die die Kontur auf wesentliche Punkte, insbesondere Eckpunkte, beschr{\"\i}{\?}ï¿½kt
	und damit fr die weitere Verarbeitung vorbereitet. Das Verfahren
	liefert eine Beschreibung der erkannten Objekte hinsichtlich ihrer
	Lage im Raum. Dazu wird die Transformation der Modelldaten in Szenendaten
	in Form von Translation und Rotation bestimmt. Diese Ergebnisdaten
	werden in einer VRML-Umgebung dargestellt und sind beispielsweise
	zur Ansteuerung eines Roboters in einer industriellen Umgebung verwendbar.
	1 http:\textbackslash\textbackslashwww.laserkamera.de 2 W.E.L. Grimson:
	Object Recognition by Compute: The Role of Geometric Constraints,
	The MIT Press, 1990. 3 Evgueni Poliak. Zur dreidimensionalen Darstellung
	von Tiefenbildern in VRML, Diplomarbeit Universit{\"\i}{\?}ï¿½ Bremen,
	2000.},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www-agki.tzi.de/grp/ag-ki/download/2001/foedischetal01.pdf}
}

