booktitle = {34th Annual German Conference on Artificial Intelligence},
editor = {Bach, Joscha and Edelkamp, Stefan},
pages = {313--324},
publisher = {Springer-Verlag},
title = {{Shape Retrieval with Qualitative Relations: The Influence of Part-Order and Approximation Precision on Retrieval Performance and Computational Effort}},
url = {http://www.arne.schuldt.info/publications-ki2011.html},
year = {2011},
