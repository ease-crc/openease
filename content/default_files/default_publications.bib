@INPROCEEDINGS{Alshutha,
  author = {Alshuth, Peter and Hermes, Thorsten and Klauck, Christoph and R{\"o}per,
	Michael},
  title = {IRIS Image retrieval for images and Videos},
  booktitle = {Proceedings of first Int. Workshop of Image Databases and Multi-Media
	Search, IDB-MMS},
  year = {1996},
  pages = {170--178},
  month = {August},
  abstract = {The large amount of available multimedia information (e.g. videos,
	audio, images) requires efficient and effective annotation and retrieval
	methods. The System IRIS (Image Retrieval for Information Systems),
	which was developed at the University of Bremen in the AI group,
	is designed for content-based retrieval of single images. Techniques
	and methods from computer vision and Artificial Intelligence are
	combined in a new way within IRIS. As videos become a more important
	role in the frame of multimedia, we want to make videos available
	for IRIS. The first step is the detection and extraction of shots
	from a video. This is done by a histogram based method. The second
	step is to combine the images in a shot to a single image. This image
	describes the shot (Mosaicing-technique) and can be analyzed by the
	the IRIS-System. IRIS has been tested on several domains, (e.g. landscape
	images, technical drawings), which cover a wide range of applications.},
  owner = {pmania},
  timestamp = {2012.11.06}
}

