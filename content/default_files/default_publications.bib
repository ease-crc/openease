@ARTICLE{Hermes2002,
  author = {Hermes, Thorsten and Miene, Andrea},
  title = {Domain-Independent Texture Analysis And Description},
  journal = {Kuenstliche Intelligenz},
  year = {2002},
  volume = {16},
  pages = {64--69},
  number = {1},
  abstract = {The problem of finding the intended image (or photography) is well
	known. Especially in the context of so-called image retrieval systems.
	Content is necessary to retrieve the intended image out of a large
	database. In order to automatically extract content from an image,
	certain features - color, texture, or contour - are needed. Texture
	is one important feature in the context of content-based image retrieval
	systems like IRIS. Particularly within the scope of a query, texture
	is a powerful criterion for searching. Therefore, a domain-independent
	texture analysis, i.e. a segmentation and classification approach
	is needed. The texture segmentation is implemented by a combination
	of region- and edge-oriented segmentation methods. The texture region
	image and the texture edges image are combined to one texture-based
	segmented image. The classification part provides an approach for
	automatic texture description based on visual texture properties.
	A set of texture features, which directly coincide with the human
	visual perception of textures, is used for a domain-independent texture
	classification. This could be used in image retrieval systems. The
	methods using visual properties were tested on natural textures as
	well as on synthetic textures. Various statistical texture features
	were evaluated and we found a direct mapping between certain statistical
	texture features and the items of the set of visual texture properties.},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www-agki.tzi.de/grp/ag-ki/download/2002/hermesmiene02.pdf}
}

