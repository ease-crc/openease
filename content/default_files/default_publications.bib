  address = {Seattle, Washington, USA},
  month = {September10--3},
  abstract = {Textual inserts and closed captures superimposed on digital videos
	often contain important and exclusive information about the video
	contents which cannot be found in the other information channels
	like the audio signal or the underlying video stream. Therefore,
	it is very helpful to extract these textual information automatically
	and add this information to a video index as it is generated by a
	video archiving and retrieval systems like e.g. ADViSOR1, AVAnTA
	or DiVA . Due to the fact that common OCR systems are restricted
	to binary images the video frames have to be preprocessed to extract
	the textual inserts from the image in the background. In this paper
	a new approach for segmentation of textual inserts from digital videos
	or images is proposed. The main difficulties are the low resolution
	and quality of digital video material, the high amount of image data,
	the very complex structured and textured background and the unknown
	color, size and position of the text to extract from the image. 1
	http://www.advisor-projekt.com},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www-agki.tzi.de/grp/ag-ki/download/2001/mieneetal01.pdf}
}

@INPROCEEDINGS{Mienef,
  author = {Miene, Andrea and Hermes, Thorsten and Ioannidis, George T.},
  title = {Automatic Video Indexing with the ADViSOR System},
  booktitle = {Proc. International Workshop on Content-Based Multimedia Indexing},
  year = {2001},
  pages = {427--433},
  month = {September19--1},
  abstract = {In this paper we present a brief overview of our work in the context
	of the ADViSOR1 project. The objective of the project is to provide
	a system both to support and simplify the performed tasks during
	the archive and to improve the efficiency of the retrieval process
	as well. The presented system comprises two modules: a) the Video
	Indexing Studio (VIS) containing the multimedia indexing methods
	and tools, which are developed and enhanced in order to perform the
	archive tasks automatically, and b) the Video Exploitation System
	(VES), which operates as the retrieval interface. The VIS allows
	the segmentation of a video into single shots, the extraction of
	representative frames and the processing of the textual inserts into
	a suitable form as an input to a standard optical character recognition
	system. The shot clustering method, which is also included into VIS,
	provides the basic scene (sequence of shots with linked up contents)
	detection functionality by permitting a semantic interpretation of
	the scene, e.g. certain sequences can lead to the conclusion that
	this specific scene is a dialogue. Furthermore an additional valuable
	component for the semantic interpretation of scenes or shots is motion
	tracking, where moving objects are detected and their motion is described.
	1 ADViSOR stands for Advanced Digital Video Storage and Online Retrieval.
	Main European partners working in collaboration with the Center of
	Computing Technologies, Image Processing Department, University of
	Bremen, at the project are: EADS, Matra Systemes {\&} Information,
	France, Technical University of Catalonia (UPC), Spain, RWTH Aachen,
	Germany and SdwestRundFunk Germany. Further Information can be obtained
	at http://www.advisor-project.com},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www-agki.tzi.de/grp/ag-ki/download/2001/mieneetal01b.pdf}
}

@INPROCEEDINGS{Miene2002c,
  author = {Miene, Andrea and Visser, Ubbo},
  title = {Interpretation of spatio-temporal relations in real-time and dynamic
	environments},
  booktitle = {RoboCup 2001: Robot Soccer World Cup V},
  year = {2001},
  editor = {Birk, Andreas and Coradeschi, Silvia and Tadokoro, Satoshi},
  number = {2377},
  series = {Lecture Notes in Computer Science},
  pages = {291--318},
  publisher = {Springer},
  abstract = {With the more sophisticated abilities of teams within the simulation
	league high level online functions become more and more attractive.
	Last year we proposed an approach to recognize the opponents strategy
	and developed the online coach accordingly. However, this approach
	gives only information about the entire team and is not able to detect
	significant situations (e.g. double pass, standard situations, repeated
	patterns between two or three players). In this paper we describe
	a new method which describes spatio-temporal relations between objects.
	In addition, this approach is able to track the objects and therefore
	the relations between them online so that we are able to interpret
	situations over time during the game. This enables us to detect the
	above mentioned situations. We can implement this in the online coach
	in order to enrich our team with high level functions. This new method
	is domain independent.},
  doi = {10.1007/3-540-45603-1{\_}57},
  keywords = {RoboCup},
  owner = {pmania},
