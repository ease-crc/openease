@ARTICLE{Timm1998,
  author = {Timm, Ingo J.},
  title = {Risiko-Scores und ihre Bedeutung in der Intensivmedizin},
  journal = {Medizin im Dialog},
  year = {1998},
  volume = {Sonderausgabe Intensivpflege},
  pages = {34--38},
  month = {November},
  abstract = {Die moderne Intensivmedizin bietet neben neuen Chancen f{\"u}r die
	Patienten auch neue Risiken, wie lebensbe-drohende Infektionen oder
	Multiorganversagen. Diese Risiken sollen durch Minimierung von verschiedenen
	{\"a}u{\ss}eren Einwirkungen reduziert werden. So ergeben sich besondere
	Anforderungen an die Hygiene, die {\"U}ber-wachung von Laborwerten
	und Organfunktionen sowie die Applikation von Pharmaka. Nach Etablierung
	der Intensivmedizin in den 60er Jahren wurden besonders in den 80er
	Jahren Risiko-Scores entwickelt, die f{\"u}r die Patienten eine individuelle
	Risikoabsch{\"a}tzung vornehmen sollten. Mittlerweile zeichnen sich
	zwei Hauptan-wendungsgebiete dieser Scores ab: zum einen werden sie
	zur Prognose und Klassifikation von Diagnose, The-rapie und Verlauf
	eingesetzt, auf der anderen Seite stellen sie eine scheinbar ad{\"a}quate
	Methode dar, um inten-sivmedizinische Leistungen zu erfassen und
	f{\"u}r die Administration zur Verf{\"u}gung zu stellen. Zun{\"a}chst
	konnten sich Risiko-Scores wegen ihrer meist sehr aufwendigen Erfassung
	nicht durchsetzen. Dieser Aufwand wird durch den verst{\"a}rkten
	Einsatz von Computern im Krankenhaus erheblich reduziert. Inzwischen
	tritt aber ein anderes Problem in den Vordergrund: Risiko-Scores
	stellen eine m{\"o}gliche Schnittstelle zwischen klinischer Diagnostik
	und administrativer Abrechnung dar und stehen somit im Brennpunkt
	{\"o}ffentlicher Kritik, da ihnen eine Schl{\"u}sselrolle bei der
	Reduktion von Behandlungskosten zugeschrieben wird. Im folgenden
	wird eine allgemeine Einf{\"u}hrung in die Thematik gegeben, die
	{"}klassischen{"} Risiko-Scores APACHE II und T.I.S.S. vorgestellt
	und die Bedeutung des Einsatzes von Risiko-Scores diskutiert. Den
	Ab-schlu{\ss} bildet eine Bewertung mit einer kurzen Checkliste f{\"u}r
	den praktischen Einsatz.},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www-agki.tzi.de/grp/ag-ki/download/1998/timm{\_}mid98{\_}paper.pdf}
}

@TECHREPORT{Timm1998b,
  author = {Timm, Ingo J.},
  title = {{Automatische Generierung von Risikoklassifikationen in der Intensivmedizin}},
  institution = {Technologie-Zentrum Informatik, Universit{\"a}t Bremen},
  year = {1998},
  type = {TZI-Bericht},
  number = {13/98},
  abstract = {Im Rahmen der Diplomarbeit {"}Automatische Generierung von Risikoklassifikationen{"}
	wurde untersucht, wie mit Hilfe des Knowledge Discovery in Databases
	(KDD) bzw. Data Minings (DM) automatisch Risikoklassifikationen in
	der Dom{\"a}ne Intensivmedizin erstellt werden k{\"o}nnen . Die Risikoklassifikationen
	sollen m{\"o}glichst als eine kompakte Regelmenge dargestellt werden,
	die auch ohne Verwendung eines Computers anwendbar ist. Die Intensivpflege,
	wie sie uns heute bekannt ist, hat sich erst in den sechziger Jahren
	etabliert . Hier werden schwerstkranke Patienten behandelt, so da{\ss}
	der Einsatz modernster Apparate und besonders starker Pharmazeutika
	notwendig ist. In diesem Rahmen sind die Patienten auch mit neuen
	Risiken, wie krankenhausintern erworbenen Infektionen und unerw{\"u}nschten
	Arzneimittelwirkungen konfrontiert. Lebensbedrohliche Lungenentz{\"u}ndungen
	oder Multi-Organversagen sind m{\"o}gliche Folgen. Seit Anfang der
	achtziger Jahre werden sogenannte Risiko-Scores entwickelt, die helfen
	sollen, ausgehend von Risikofaktoren individuelle Risiken f{\"u}r
	den Patienten zu bestimmen. Hierbei wird versucht, das {\"U}berleben,
	die Dauer des Aufenthaltes auf der Intensivstation, Multi-Organversagen
	o.{\"a}. vorherzusagen. Im Rahmen dieser Arbeit wurde untersucht,
	wie auf der Basis real erhobener Patientendaten automatisch Risikoklassifikationen
	zur Pr{\"a}diktion der Sterbewahrscheinlichkeit, der Entwicklung
	einer Infektion und der Vorhersage der Liegedauer erstellt werden
	k{\"o}nnen. Um diese Aufgabe ad{\"a}quat zu l{\"o}sen, bieten sich
	Konzepte und Methoden aus den Bereichen Statistik, K{\"u}nstliche
	Intelligenz (KI) und Neuroinformatik an. Viele L{\"o}sungsans{\"a}tze
	finden sich im besonderen in der Mathematik, wie Multivariate Datenanalyse,
	Clustering und Spektralanalyse. Die Informatik stellt L{\"o}sungsmethoden
	aus der KI f{\"u}r die Klassifikation und Vorhersage im Bereich des
	Maschinellen Lernens (ML), des DM und des KDD zur Verf{\"u}gung.
	Die Grundlage bildet das DM in dem sowohl Methoden der KI (ML) als
	auchder Neuroinformatik und Mathematik Anwendung finden. Es werden
	drei Methoden, n{\"a}mlich Entscheidungsbaumlernen, Klassifikation
	durch ein Neuronales Netzwerk und Lineare Diskriminanzanalyse exemplarisch
	angewendet und auf ihre Vor- und Nachteile in Bezug auf das gegebene
	Problem untersucht.},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www-agki.tzi.de/grp/ag-ki/download/1998/timm{\_}tzi1398{\_}paper.pdf}
