@INPROCEEDINGS{Hermesd,
  author = {Hermes, Thorsten and Miene, Andrea and Kreyenhop, P.},
  title = {On Textures: A Sketch of a Texture-Based Image Segmentation Approach},
  booktitle = {Classification and Information Processing at the Turn of the Millenium},
  year = {2000},
  editor = {Decker, R. and Gaul, W.},
  pages = {210--218},
  month = {March1--2},
  note = {Proc. 23rd Annual Conference Gesellschaft f{\"u}r Klassifikation
	e.V. 1999},
  abstract = {In the context of this work we present a new approach of domain-independent
	texture segmentation. The texture segmentation is implemented by
	a combination of region and edge-oriented segmentation methodes.
	The region-oriented segmentation procedure is based on texture energy
	measures . For each pixel an energy value is calculated, which serves
	as the homogeneity criterion. The edge-oriented methode measures
	texture edges by energy distributions in certain directions . The
	texture edges serve then as discontinuity criterion for the blob
	coloring algorithm adapted to grey values. The texture region image
	and the texture edges image are combined to one texture-based segmented
	image.},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www-agki.tzi.de/grp/ag-ki/download/2000/hermesetal00b.pdf}
