@ARTICLE{Lutz2008,
  author = {Lutz, Michael and Sprado, J{\"o}rn and Klien, Eva and Schubert, Chris
	and Christ, Ingrid},
  title = {Overcoming Semantic Heterogeneity in Spatial Data Infrastructures},
