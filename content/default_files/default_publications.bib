@INPROCEEDINGS{Ranze2002a,
  author = {Ranze, K. Christoph and Scholz, Thorsten and Wagner, Thomas and G{\"u}nter,
	Andreas and Herzog, Otthein and Hollmann, Oliver and Schlieder, Christoph
	and Arlt, Volker},
  title = {A {S}tructure {B}ased {C}onfiguration {T}ool: {D}rive {S}olution
	{D}esigner - {DSD}},
  booktitle = {IAAI02},
  year = {2002},
  editor = {Chien, Steve and Riedl, John},
  pages = {845--852},
  address = {Edmonton, Alberta, Canada},
  month = {July28--August1},
  publisher = {AAAI Press},
  abstract = {In this paper, we describe the configuration tool Drive Solution Designer
	(DSD). The DSD is used by sales engineers of the company Lenze AG
	(www.lenze.com) for the configuration of complex drive systems in
	order to make on-site offers together with the customer. The aim
	of this process is to generate a consistent solution which fulfills
	the functional requirements of the user along with optimization criteria
	such as price and delivery time. The preparation of a technical offer
	requires fundamental knowledge of complex physical and in particular
	technical correlations of drive components, in depth knowledge of
	the product catalog as well as high empirical knowledge about the
	order of the parameterization of the components. In order to meet
	these requirements knowledge-based AI-techniques are required. In
	the DSD we use a domain independent incremental structure-based configuration
	approach with different knowledge representation mechanisms and a
	sophisticated declarative control. Currently DSD is used with great
	success by approx. 150 sales engineers of the company Lenze for the
	design layout task. The introduction of the DSD lead to a drastic
	time reduction for drive solution development and reduces incorrect
	solutions to nearly 0 percent.},
  isbn = {978-0-262-51129-2},
  owner = {pmania},
  timestamp = {2012.11.06}
}

@TECHREPORT{Rugge2002,
  author = {R{\"u}gge, Ingrid},
  title = {Studie {"}Technologische und anwendungsorientierte Potenziale mobiler,
	tragbarer Computersysteme{"}},
  institution = {Technolgie-Zentrum Informatik (TZI), Universit{\"a}t Bremen},
  year = {2002},
  type = {TZI-Bericht},
  number = {24},
  abstract = {Papierlose elektronische Datenerfassung bei der Inspektion von Industrieanlagen
	vor Ort, virtuelle Reisef{\"u}hrer, die die unmittelbare Umgebung
	erkl{\"a}ren, oder multimediale Reparaturanleitungen direkt am zu
	reparierenden Ger{\"a}t sind keine wilden Utopien mehr, sondern handfeste
	Anwendungsbeispiele f{\"u}r Wearable-Computing-L{\"o}sungen. Eine
	umfangreiche Studie liefert nun Einzelheiten zu eingesetzten und
	verf{\"u}gbaren technologischen Komponenten, die derartige ?mobile?
	T{\"a}tigkeiten ? jenseits von Schreibtisch und B{\"u}ro ? unterst{\"u}tzen.
	Und sie gibt einen umfassenden {\"U}berblick {\"u}ber Anwendungsbereiche,
	in denen diese neuen Technologien gewinnbringend eingesetzt werden
	k{\"o}nnen, sowie {\"u}ber Anwendungsf{\"a}lle, f{\"u}r die es bereits
	Realisierungen gibt. Der Begriff ?mobiles, tragbares Computersystem?
	wird heute sehr breit gefasst und bezeichnet alles, was sich zwischen
	Notebook, Handy und intelligentem Stoff bewegt. Die Studie konzentriert
	sich innerhalb dieser Bandbreite auf das noch junge, aber wirtschaftlich
	relevante und zukunftstr{\"a}chtige Paradigma ?Wearable Computing?.
	?Mobil und tragbar? wird mit ?wearable? gleichgesetzt und bezeichnet
	das ?Anziehen? von Computerhardware: Der Rechner wird zusammen mit
	seinen Ein-/Ausgabemedien so am K{\"o}rper getragen, dass beide H{\"a}nde
	f{\"u}r andere T{\"a}tigkeiten frei bleiben und der Computer in der
	Bewegung genutzt werden kann. Diese neuen Eigenschaften von Hardware
	erm{\"o}glichen den Einsatz von Computersystemen in Situationen und
	an Orten, die bisher f{\"u}r eine Computerunterst{\"u}tzung unzug{\"a}nglich
	waren. Sie er{\"o}ffnen dar{\"u}ber hinaus ein weites Feld f{\"u}r
	die Entwicklung von neuen, innovativen Softwaresystemen, die die
	Mobilit{\"a}t und die Individualisierbarkeit derartiger Systeme f{\"u}r
	neue Funktionalit{\"a}ten und f{\"u}r ein neues Paradigma der Interaktion
	zwischen Mensch und Computer nutzen. Auf {\"u}ber 300 Seiten werden
	mobile Technologien und Anwendungsbeispiele beschrieben. Recherchiert
	wurden mehr als 30 Wearable Computer und Smart Clothings sowie {\"u}ber
	40 mobile Eingabemedien wie z.B. Unterarmtastaturen, drahtlose Pointing-Devices,
	robuste Datenhandschuhe und Sensoren. Als Ausgabeger{\"a}te f{\"u}r
	mobile, tragbare Computersysteme stehen heute in erster Linie Mikrodisplays
	und Head-Mounted-Displays (HMDs) zur Verf{\"u}gung, von denen in
	der Studie mehr als 25 schematisch charakterisiert werden. Zu all
	diesen Produkten steht im Anhang eine Addressliste der Firmen zur
	Verf{\"u}gung. Neben der Bestandsaufnahme aktueller technischer Komponenten
	als Grundlage f{\"u}r die Entwicklung innovativer mobiler L{\"o}sungen
	sind die ann{\"a}hernd 50 dokumentierten Anwendungsbeispiele ein
	weiteres Highlight der Studie. Sie vermitteln in nachhaltiger Weise
	einen Eindruck davon, wo Wearable Computing bzw. Mobile Solutions
	f{\"u}r die AnwenderInnen einen Mehrwert liefern. Die Breite und
	die Vielfalt der Beispiele regen die Phantasie an bei der Suche nach
	Innovationspotenzialen und zukunftstr{\"a}chtigen mobilen L{\"o}sungen.},
  location = {Bremen},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www.tzi.de/~ruegge/PDF-Dateien/TZI-Bericht24{\_}3.pdf}
}

