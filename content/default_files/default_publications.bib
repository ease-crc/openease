  booktitle = {Proceedings of 5th AGILE Conference on Geographic Information Science},
  year = {2002},
  editor = {Ruiz, M. and Gould, M. and Ramon, J.},
  pages = {279--289},
  address = {Palma de Mallorca, Spain},
  abstract = {Geo-referenced data play an increasingly important role in many application
