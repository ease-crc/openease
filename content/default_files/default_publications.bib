@INPROCEEDINGS{Redeke,
  author = {Redeke, Ingo},
  title = {Image and Graphic Reader},
  booktitle = {ICIP2001},
  year = {2001},
  abstract = {The Image {\&}amp; Graphic Reader (IGR) is an assisting tool to retrieve
	information from business charts and present it to blind computer
	users. Business charts like bar-, pie- or block charts occurring
	in newspapers and web sites are analyzed by standard image processing
	methods. Common segmentation algorithms produce homogeneous areas
	and contour lines that are combined to components characterizing
	a specific chart type. Since the component's geometric features encode
	the tabular content of the chart, their dimensions are measured against
	a certain scale. This scale is either explicitly defined in the image,
	like scales of a previously identified axis or is intrinsic to the
	type of the chart (e.g. angles in pie charts). After the primal classification
	the extracted tabular data is collected and analyzed systematically
	to produce a representation that subdivides a chart into predefined
	levels. These levels are presented through verbal descriptions. A
	blind user can thus navigate through the levels by keyboard and the
	spoken feedback of a speech synthesizer.},
  owner = {pmania},
  timestamp = {2012.11.06}
}

@INPROCEEDINGS{Redekea,
  author = {Redeke, Ingo},
  title = {Hierarchical Interpretation of Business Charts for Blind Computer
	Users using UML},
  booktitle = {GREC01},
  year = {2001},
  abstract = {Two interlinked hierarchical concepts for decomposing business charts
	and a subsequent presentation of the containing information to blind
	people have been developed. The decomposition for each specific chart
	type is given by a UML diagram that describes components as associated
	classes. Necessary topological relations are added as constraint
	notes to an association. Potential inclusion of one component is
	marked by a cardinality range beginning at 0. We can show that the
	complexity for chart identification and the classification of its
	type can be reduced to combinations of small numbers of simple standardized
	items, e.g. rectangles or pie segments by using UML diagrams. An
	image processing routine would thus need to extract simple atomic
	objects whose geometrical relations are then piped into a topology
	classifier. Since the presentation stage is originally directed at
	blind persons, it contains levels of verbally described chart features.
	Separating these levels thus reflects the power of the human working
	memory. Any information retrieved from a chart is dominated by its
	tabular data. Thus we first concentrate on how the intrinsic table
	of related values would be compiled and reduced for verbalization.
	Secondary features such as descriptive statistics, layout, or design
	would be included at different levels of presentation.},
  owner = {pmania},
  timestamp = {2012.11.06}
}

