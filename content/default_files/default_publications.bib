@article{Heuten2006b,
abstract = {In this article, we describe an approach to provide access to geographic information for multimodal non-visual interaction. We use the Geography Markup Language (GML) to describe geographic data and to conserve the semantic knowledge about geographic features. The description can serve as an input to innovative presentations of spatial structures using haptic and auditory interfaces. To be able to present existing geographic information in a multimodal manner, the data in Geographic Information Systems (GIS) and bitmap images has to be transformed to GML. For data stored in GIS, this can easily be performed. For geographic information encoded in bitmap images, we extract geographic features from these images automatically based on color information and model them in GML. Finally an annotation tool enables the publisher to add individual information to maps on the basis of GML. We show exemplarily how our process can be applied to provide access to city maps for blind and visually impaired people.}
,
author = {Heuten, Wilko and Horstmann, Mirko and Boll, Susanne and Miene, Andrea},
journal = {eMinds - International Journal on Human-Computer Interaction},
keywords = {ISPL,Accessibility,GIS,GML,auditory,geography,haptic},
number = {2},
pages = {75--92},
title = {{Geographic Information Modeling for Multimodal Interaction}},
volume = {1},
year = {2006}
}

@INPROCEEDINGS{Horstmann2006,
  author = {Horstmann, Mirko and Heuten, Wilko and Boll, Susanne and Miene, Andrea},
  title = {Automatic Annotation of Geographic Maps},
  booktitle = {Computers Helping People with Special Needs, 10th International Conference},
  year = {2006},
  series = {1},
  pages = {69--76},
  address = {Linz, {\"O}sterreich},
  month = {July/July},
  publisher = {Klaus Miesenberger},
  abstract = {In this paper, we describe an approach to generate semantic descriptions
	of entities in city maps so that they can be presented through accessible
	interfaces. The solution we present processes bitmap images containing
	city map excerpts. Regions of interest in these images are extracted
	automatically based on colour information and subsequently their
	geometric properties are determined. The result of this process is
	a structured description of these regions based on the Geography
	Markup Language (GML), an XML based format for the description of
	GIS data. This description can later serve as an input to innovative
	presentations of spatial structures using haptic and auditory interfaces.},
  keywords = {Accessibility,geography,GML,haptic,auditory,GIS},
  owner = {pmania},
  timestamp = {2012.11.06}
}

@INPROCEEDINGS{Jacobs2006a,
  author = {Jacobs, Arne},
  title = {Using Self-similarity Matrices for Structure Mining on News Video},
  booktitle = {Proceedings of the 4th Hellenic Conference on Artificial Intelligence
	(SETN'06)},
  year = {2006},
  editor = {Antoniou, Grigoris and Potamias, George and Spyropoulos, Costas and
	Plexousakis, Dimitris},
  month = {May},
  publisher = {Springer-Verlag},
  abstract = {Video broadcast series like news or magazine broadcasts usually expose
	a strong temporal structure, along with a characteristic audio-visual
	appearance. This results in frequent patterns occurring in the video
	signal. We propose an algorithm for the automatic detection of such
	patterns that exploits the video's self-similarity induced by the
	patterns. The approach is applied to the problem of anchor shot detection,
	but can also be used for other related purposes. Tests on real-world
	video data show that it is possible with our method to detect anchor
	shots fully automatically with high reliability.},
  owner = {pmania},
  timestamp = {2012.11.06},
  url = {http://www.tzi.de/~jarne/jacobs06.pdf}
}

